{
  "name": "significa.co",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run datasources && npm run sitemap",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:unit": "vitest",
    "lint": "prettier --check . && eslint .",
    "validate": "npm run check && npm run lint",
    "format": "prettier --write .",
    "sb:login": "npx storyblok@latest logout && npx storyblok login",
    "sb:pull": "npx storyblok@latest pull-components --space=198185",
    "sb:types": "npx storyblok-generate-ts@latest source=./components.198185.json target=./src/types/bloks.d.ts && prettier ./src/types/bloks.d.ts --write",
    "sb": "npm run sb:pull && npm run sb:types",
    "datasources": "svelte-kit sync && npx ts-node@latest --project ./node.tsconfig.json ./scripts/datasources.ts",
    "sitemap": "svelte-kit sync && npx ts-node@latest --project ./node.tsconfig.json ./scripts/sitemap.ts",
    "prepare": "husky"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.5.2",
    "@sveltejs/kit": "^2.12.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/matter-js": "^0.19.8",
    "@types/prismjs": "^1.26.5",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^1.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "husky": "^9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prismjs": "^1.29.0",
    "svelte": "^4.2.19",
    "svelte-check": "^4.1.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.16",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^4.5.5",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.712.0",
    "@aws-sdk/client-s3": "3.712.0",
    "@aws-sdk/client-ses": "3.712.0",
    "@aws-sdk/s3-request-presigner": "3.712.0",
    "@melt-ui/svelte": "^0.86.2",
    "@notionhq/client": "^2.2.15",
    "@sentry/sveltekit": "^7.120.2",
    "@significa/svelte-ui": "^0.0.34",
    "@storyblok/js": "^3.1.9",
    "matter-js": "^0.20.0",
    "posthog-js": "^1.201.0",
    "prism-svelte": "^0.5.0",
    "svelte-confetti": "^1.4.0"
  }
}

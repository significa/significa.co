{
  "name": "significa.co",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run datasources && npm run sitemap",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:unit": "vitest",
    "lint": "prettier --check . && eslint .",
    "validate": "npm run check && npm run lint",
    "format": "prettier --write .",
    "sb:login": "npx storyblok@latest logout && npx storyblok login",
    "sb:pull": "npx storyblok@latest pull-components --space=198185",
    "sb:types": "npx storyblok-generate-ts@latest source=./components.198185.json target=./src/types/bloks.d.ts && prettier ./src/types/bloks.d.ts --write",
    "sb": "npm run sb:pull && npm run sb:types",
    "datasources": "svelte-kit sync && npx ts-node@latest --project ./node.tsconfig.json ./scripts/datasources.ts",
    "sitemap": "svelte-kit sync && npx ts-node@latest --project ./node.tsconfig.json ./scripts/sitemap.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.1.0",
    "@sveltejs/kit": "^1.30.4",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/matter-js": "^0.18.5",
    "@types/prismjs": "^1.26.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.44.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prismjs": "^1.29.0",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.13",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vite": "^4.5.5",
    "vite-plugin-mkcert": "^1.17.6",
    "vitest": "^0.33.0"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.658.1",
    "@aws-sdk/client-s3": "3.413.0",
    "@aws-sdk/client-ses": "3.413.0",
    "@aws-sdk/s3-request-presigner": "3.413.0",
    "@melt-ui/svelte": "^0.70.0",
    "@notionhq/client": "^2.2.15",
    "@sentry/sveltekit": "^7.119.1",
    "@significa/svelte-ui": "^0.0.34",
    "@storyblok/js": "^2.3.0",
    "matter-js": "^0.19.0",
    "posthog-js": "^1.165.0",
    "prism-svelte": "^0.5.0",
    "svelte-confetti": "^1.4.0"
  }
}
